black;\n}\n.toggleMenu__side :last-child {\n    border: 0;\n    margin-top: 1px;\n    border-top: 1px double black;\n}\n.toggleMenu__side__item--left {\n    border-radius: 7px 0;\n}\n.toggleMenu__side__item--right {\n    border-radius: 0 7px;\n}\n.toggleMenu__text {\n    white-space: nowrap;\n}\n.navbar-menu-wrapper {\n    max-height: 0;\n    overflow: hidden;\n}\n.navbar-menu {\n    border-bottom: var(--thin-border);\n    text-transform: uppercase;\n    font-family: ImpactLabelReversed;\n    font-size: xx-large;\n}\n\n.navbar-menu__item {\n    position: relative;\n    color: var(--text-color);\n    margin: auto;\n}\n\n.navbar-menu__item > a {\n    position: relative;\n    z-index: 2;\n}\n\n.main-content {\n    background: transparent;\n}\n.first-content {\n    display: flex;\n    flex-direction: column;\n    justify-content: space-between;\n    align-items: center;\n    margin-bottom: 24px;\n}\n\n/* disable line drawing around nav items on mobile */\n.svg-container {\n    display: none;\n}\n\n.heading {\n    font-family: Heading;\n    text-transform: capitalize;\n    font-size: xxx-large;\n    width: 100%;\n    text-align: center;\n    padding: 8px 0;\n    border-bottom: var(--thinnest-border);\n}\n\n.section-heading {\n    font-family: ImpactLabelReversed;\n    font-size: xx-large;\n    text-align: center;\n    border: var(--thin-border);\n    border-left: 0;\n    border-right: 0;\n}\n\n.section-content {\n    margin-top: 24px;\n    margin-bottom: 24px;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-between;\n}\n.centered-content {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n}\n.section-content__text-block {\n    max-width: 420px;\n    padding: 8px;\n}\n@media only screen and (min-width: 620px) {\n    #toggleMenu {\n        display: none;\n    }\n    .navbar-menu-wrapper {\n        overflow: visible;\n        max-height: none;\n    }\n    .navbar-menu {\n        border-top: var(--thin-border);\n    }\n    .navbar-menu__item--circle {\n        fill: none;\n        stroke: #9b1c1c;\n        stroke-width: 4px;\n        opacity: 80%;\n    }\n    .navbar-menu__item--circle-1 {\n        stroke-dasharray: 425.6;\n        stroke-dashoffset: 426;\n    }\n    .navbar-menu__item--circle-2 {\n        stroke-dasharray: 419.8;\n        stroke-dashoffset: 420;\n        stroke: #092768;\n        opacity: 80%;\n    }\n    .navbar-menu__item--circle-3 {\n        stroke-dasharray: 515.8;\n        stroke-dashoffset: 516;\n        stroke: #093f33;\n        opacity: 80%;\n    }\n    .svg-container {\n        display: initial;\n        z-index: 1;\n        position: absolute;\n    }\n    .svg-container1 {\n        top: -75px;\n        left: -260px;\n        width: 600px;\n        height: 200px;\n    }\n    .svg-container2 {\n        top: -70px;\n        left: -210px;\n        width: 400px;\n        height: 200px;\n    }\n    .svg-container3 {\n        top: -70px;\n        left: -260px;\n        width: 500px;\n        height: 200px;\n    }\n    .first-content {\n        flex-direction: row;\n    }\n    .section-content {\n        flex-direction: row;\n    }\n}\n"},"lineCount":null}},"error":null,"hash":"7fd1ba61982256eaad2b645ce58ca852","cacheData":{"env":{}}}