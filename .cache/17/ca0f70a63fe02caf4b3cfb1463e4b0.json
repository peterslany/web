lumn":21}},{"source":"newspaper.css","name":null,"original":{"line":157,"column":4},"generated":{"line":157,"column":4}},{"source":"newspaper.css","name":null,"original":{"line":158,"column":0},"generated":{"line":158,"column":0}}],"sources":{"newspaper.css":":root {\n    --text-color: rgb(59, 59, 59);\n    --thin-border: 2px solid var(--text-color);\n    --thick-border: 5px solid var(--text-color);\n}\n\n@font-face {\n    font-family: ImpactLabelReversed;\n    src: url(\"../assets/fonts/impact_label/impact_label_reversed.ttf\");\n}\n\n@font-face {\n    font-family: BonVoyage;\n    src: url(\"../assets/fonts/made_bon_voyage/bon_voyage.otf\");\n}\n\nbody {\n    background-image: url(\"../assets/images/ep_naturalwhite.png\");\n    background-size: auto;\n    background-repeat: repeat;\n}\nimg {\n    filter: saturate(0%) contrast(130%);\n    border: var(--thick-border);\n    transition: filter 500ms ease-in-out;\n}\nimg:hover {\n    filter: saturate(20%) contrast(160%);\n    transition: filter 500ms ease-in-out;\n}\np {\n    font-family: BonVoyage;\n    font-weight: 500;\n    font-size: large;\n}\n.header {\n    color: var(--text-color);\n    margin-bottom: 0;\n    font-family: ImpactLabelReversed;\n}\n#toggleMenu {\n    font-family: ImpactLabelReversed;\n    font-size: xx-large;\n    border-top: var(--thin-border);\n    border-bottom: var(--thin-border);\n    text-align: center;\n    display: flex;\n    cursor: pointer;\n}\n.toggleMenu__side {\n    width: 100%;\n}\n.toggleMenu__side__item {\n    height: calc(50% - 1px);\n    border-bottom: 1px double black;\n}\n.toggleMenu__side :last-child {\n    border: 0;\n    margin-top: 1px;\n    border-top: 1px double black;\n}\n.toggleMenu__side__item--left {\n    border-radius: 25px 0;\n}\n.toggleMenu__side__item--right {\n    border-radius: 0 25px;\n}\n.toggleMenu__text {\n    white-space: nowrap;\n}\n.navbar-menu-wrapper {\n    max-height: 0;\n    overflow: hidden;\n}\n.navbar-menu {\n    border-bottom: var(--thin-border);\n    text-transform: uppercase;\n    font-family: ImpactLabelReversed;\n    font-size: xx-large;\n}\n\n.navbar-menu__item {\n    position: relative;\n    color: var(--text-color);\n    margin: auto;\n}\n\n.navbar-menu__item > a {\n    position: relative;\n    z-index: 2;\n}\n\n.main-content {\n    background: transparent;\n}\n\n/* disable line drawing around nav items on mobile */\n.svg-container {\n    display: none;\n}\n\n@media only screen and (min-width: 620px) {\n    #toggleMenu {\n        display: none;\n    }\n    .navbar-menu-wrapper {\n        overflow: visible;\n        max-height: none;\n    }\n    .navbar-menu {\n        border-top: var(--thin-border);\n    }\n    .navbar-menu__item--circle {\n        fill: none;\n        stroke: #9b1c1c;\n        stroke-width: 4px;\n        opacity: 80%;\n    }\n    .navbar-menu__item--circle-1 {\n        stroke-dasharray: 425.6;\n        stroke-dashoffset: 426;\n    }\n    .navbar-menu__item--circle-2 {\n        stroke-dasharray: 419.8;\n        stroke-dashoffset: 420;\n        stroke: #092768;\n        opacity: 80%;\n    }\n    .navbar-menu__item--circle-3 {\n        stroke-dasharray: 515.8;\n        stroke-dashoffset: 516;\n        stroke: #093f33;\n        opacity: 80%;\n    }\n    .svg-container {\n        display: initial;\n        z-index: 1;\n        position: absolute;\n    }\n    .svg-container1 {\n        top: -70px;\n        left: -250px;\n        width: 600px;\n        height: 200px;\n    }\n    .svg-container2 {\n        top: -65px;\n        left: -205px;\n        width: 400px;\n        height: 200px;\n    }\n    .svg-container3 {\n        top: -70px;\n        left: -250px;\n        width: 500px;\n        height: 200px;\n    }\n}\n"},"lineCount":null}},"error":null,"hash":"536bb2c9d8063647dced9044fe33803b","cacheData":{"env":{}}}